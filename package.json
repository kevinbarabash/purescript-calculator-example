{
  "name": "react-basic",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  },
  "scripts": {
    "build": "purs compile 'bower_components/*/src/**/*.purs' 'src/**/*.purs'",
    "watch": "pulp --watch build",
    "bundle:pulp": " pulp build --optimise --to docs/bundle.pulp.js",
    "bundle:puree": "puree --outdir docs --name bundle.puree.js output",
    "bundle:purs": "purs bundle output/**/*.js -o docs/bundle.purs.js -m Main",
    "bundle:rollup": "rollup -c",
    "webpack:server": "webpack-dev-server --progress --inline --hot"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/generator": "^7.3.4",
    "@babel/parser": "^7.3.4",
    "@babel/traverse": "^7.3.4",
    "@babel/types": "^7.3.4",
    "babel-loader": "^8.0.5",
    "mkdirp": "^0.5.1",
    "purs-loader": "^3.4.0",
    "react-hot-loader": "^4.8.0",
    "rollup": "^1.6.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-purs": "^1.0.38",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-uglify": "^6.0.2",
    "tmp": "^0.0.33",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
